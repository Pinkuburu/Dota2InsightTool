<Window x:Class="Dota2InsightTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dota2InsightTool"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel Grid.ColumnSpan="2" LastChildFill="False">
            <Border BorderThickness="1" BorderBrush="Black" Width="100">
                <Button x:Name="bTestApiCall" Content="Test Lookup" Height="59" VerticalAlignment="Top" Margin="-1,50,1,0" Click="Button_Click"/>
            </Border>
            <TextBox x:Name="tMainTextBox" TextWrapping="Wrap" Width="699"/>
        </DockPanel>

    </Grid>
</Window>
